<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
"http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<style type="text/css" media="screen">
	body{
		background: #FFF url(/images/bg.jpg) repeat-y scroll top left;
	}
</style>
</head>
<body leftmargin="0" topmargin="0" marginwidth="0" marginheight="0">
<table width="8" height="100%" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td width="8" background="/images/bg.jpg" valign="top" height="600">
    <table width="100%" border="0" cellpadding="0" cellspacing="0">

        <tr>
          <td valign="top">
          <img id="imgShow" src="/images/left.jpg" title="隐藏导航" style="cursor:pointer"   onmouseover="mouseover()" onmouseout="mouseout()"  onclick="openClose()">
          </td>
        </tr>
      </table></td>
  </tr>
</table>
<script language="javascript">
var status = 0;
var imgShow = document.getElementById('imgShow');
var op_frame = parent.window.document.getElementById('op_frame');
function openClose(){
   if (status) {
      op_frame.cols = '150,8,*';
			imgShow.src = "/images/left/right.jpg";
			imgShow.title = "显示导航" ;
      status = 0;
   } else {
	    op_frame.cols = '0,8,*';
			imgShow.src = "/images/left/right.jpg";
			imgShow.title = "显示导航" ;
			status = 1;
   }
}
      
function mouseover() {
   if (status) {
      imgShow.src = "/images/left/right2.jpg";
   } else {
      imgShow.src = "/images/left/left2.jpg";
   } 
}
      
function mouseout() {
   if (status) {
      imgShow.src = "/images/left/right.jpg";
   } else {
      imgShow.src = "/images/left/left.jpg";
   } 
}
</script>
</body>
</html>


